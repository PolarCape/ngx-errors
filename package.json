{
  "name": "@polarcape/ngx-errors",
  "version": "1.4.1",
  "description": "A declarative validation module for reactive forms",
  "author": "Ultimate Angular",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/PolarCape/ngx-errors.git",
    "type": "git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run copy && yarn run compile",
    "clean": "rimraf dist && mkdir -p dist",
    "compile": "yarn run ngc && yarn run rollup",
    "copy": "cp src/package.json dist/package.json && cp README.md dist/README.md",
    "example": "webpack-dev-server --inline --hot",
    "prestart": "yarn run clean && yarn run copy",
    "start": "nodemon --exec yarn run compile -e ts --watch src",
    "rollup": "rollup -c rollup.config.js >/dev/null 2>&1",
    "ngc": "ngc -p tsconfig.json",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "devDependencies": {
    "@angular/compiler": "6.1.9",
    "@angular/compiler-cli": "6.1.9",
    "@types/jasmine": "2.8.9",
    "@types/karma": "3.0.0",
    "@types/node": "^10.11.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "jasmine-core": "3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.5",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.5",
    "sass-loader": "^7.1.0",
    "typescript": "~2.9.2",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@angular/animations": "6.1.9",
    "@angular/common": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "core-js": "2.5.7",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "tslib": "1.9.3",
    "zone.js": "0.8.26"
  }
}
